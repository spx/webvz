<table id="rounded-corner">
<th> Parameter </th> <th> </th><th> Barrier </th> <th> limit</th>

<tr>
	<td> Number of TCP Sockets </td> 
	<td> : </td> 
	<% v = @conf_file["NUMTCPSOCK"].split(":") %>
	<td> <%= text_field "conf", "NUMTCPSOCK1", :value => v[0].gsub("\"", ""), :size => "20"%> </td>
	<td> <%= text_field "conf", "NUMTCPSOCK2", :value => v[1].gsub("\"", ""), :size => "20"%> </td>
</tr>
